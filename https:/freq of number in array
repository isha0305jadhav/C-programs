#include <stdio.h>

void cnt_freq(int arr[], int n) 
{
    int visited[n]; 

    for (int i = 0; i < n; i++) 
    {
        visited[i] = 0;
    }

    for (int i = 0; i < n; i++) {
        
        if (visited[i] == 1) continue;

        int count = 1; 

        
        for (int j = i + 1; j < n; j++) {
            if (arr[i] == arr[j]) {
                count++;
                visited[j] = 1; 
            }
        }

        
        printf("%d occurs %d times\n", arr[i], count);
    }
}

int main() {
    int n;
    printf("Enter array size: ");
    scanf("%d", &n);

    int arr[n];
    printf("Enter %d elements: ", n);
    for (int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

    cnt_freq(arr, n);
    return 0;
}
