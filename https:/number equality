#include <stdio.h>

int main()
{
    int num1,num2;
    printf("Enter a two numbers to be check:");
    scanf("%d %d",&num1, &num2);
    if(num1^num2==0)
    {
        printf("numbers are equal.");
    }
    else{
        printf("numbers are not equal.");
    }
    return 0;
}
