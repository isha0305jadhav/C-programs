#include <stdio.h>
int second_max(int arr[], int n)
{
    
    for(int i=0;i<n-1;i++)
    {
        for(int j=0;j<n-1-i;j++)
        {
            if(arr[j]<arr[j+1])
            
            {
                int temp= arr[j];
                arr[j]=arr[j+1];
                arr[j+1]=temp;
            }
        }
        
    }
    return arr[1];
    
}

int main()
{
    int n;
    printf("Enter a number of elements in the array:");
    scanf("%d",&n);
    int array[n];
    printf("\n Enter %d number of elemets in array:", n);
     for(int i=0;i<n;i++)
    {
        scanf("%d", &array[i]);
    }
    printf("\n Second largest number in the array is: %d",second_max(array,n));
   
    

    return 0;
}
